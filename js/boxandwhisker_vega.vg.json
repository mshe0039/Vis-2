{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "A parallel coordinates plot",
  "background":"bisque",
  "data": {
    "url": "https://raw.githubusercontent.com/mshe0039/Visualisation-2/main/2002-03%20NBA%20-%20Sheet1.csv"
  },
  "width": 300,
  "height": 350,
  "mark": {
    "type": "boxplot",
    "extent": "min-max",
    "size": 50
  },
  "params": [{
    "name": "select_stat",
    "value":
  }],
  "encoding": {
    "x": {
      "field": "Pos",
      "type": "nominal"
    },
    "color": {
      "field": "Pos"
    },
    "y": {
      "field": "PTS",
      "type": "quantitative"
    }
  },
  "transform": [
    {
      "filter": {
        "field": "Pos",
        "oneOf": [
          "SF",
          "SG",
          "PF",
          "PG",
          "C",
          "SG-PG"
        ]
      }
    }
  ]
}